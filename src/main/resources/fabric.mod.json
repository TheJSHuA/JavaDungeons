{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "JavaDungeons",
  "description": "A mod that adds the custom blocks from Minecraft Dungeons into Minecraft: Java Edition.",
  "authors": [
    "J0SH"
  ],
  "license": "MIT",
  "icon": "assets/dungeons/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "j0sh.javadungeons.JavaDungeons"
    ],
    "client": [
      "j0sh.javadungeons.JavaDungeonsClient"
    ]
  },

  "mixins": [
    "mixins.dungeons.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.15.x"
  },
  "suggests": {
    "flamingo": "*"
  }
}
